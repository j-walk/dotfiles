Xcursor.theme         : Bluecurve

URxvt.perl-ext-common : default,matcheri,resize-font

URxvt*keysym.C-Delete: perl:matcher:last
URxvt*keysym.M-Delete: perl:matcher:list
URxvt*matcher.rend.0: Uline Bold fg5
URxvt*depth           : 32
URxvt*buffered        : true
URxvt*shading         : 30
URxvt*fading          : 10%
URxvt*internalBorder  : 5
URxvt*scrollBar       : false
URxvt*pointerBlank    : true
URxvt*font            : xft:Monospace:size=8
URxvt*urlLauncher     : /usr/bin/firefox
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt*matcher.button  : 1
URxvt*termName        : rxvt-unicode-256color
URxvt*iso14755        : false
URxvt*iso14755_52     : false
URxvt*colorUL         : #4682B4

URxvt*resize-font.smaller: C-Down
URxvt*resize-font.bigger: C-Up

! special
URxvt*.foreground:   #e6e1dc
URxvt*.background:   #000000
URxvt*.cursorColor:  #e6e1dc

!black
*color0 : #222222
*color8 : #454545
!red
*color1 : #B22222
*color9 : #FA8072
!green
*color2 : #556B2F
*color10: #779A3E
!yellow
*color3 : #B8860B
*color11: #DAA520
!blue
*color4 : #4682B4
*color12: #6495ED
!magenta
*color5 : #9932CC
*color13: #DA70D6
!cyan
*color6 : #87CEEB
*color14: #B0E0E6
!white
*color7 : #C0C0C0
*color15: #FFFFFF

rofi.color-enabled: true
rofi.color-window: #2f343f, #002b36, #2f343f
rofi.color-normal: #2f343f, #f3f4f5, argb:0000000, #2f343f, #71ddff
rofi.color-active: #fdf6e3, #268bd2, #eee8d5, #268bd2, #fdf6e3
rofi.color-urgent: #fdf6e3, #dc322f, #eee8d5, #dc322f, #fdf6e3
