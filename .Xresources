URxvt*font                : xft:Monospace:size=8

Xcursor*theme             : Bluecurve

URxvt*perl-ext-common     : default,matcheri,resize-font

URxvt*keysym.C-Delete     : perl:matcher:last
URxvt*keysym.M-Delete     : perl:matcher:list
URxvt*matcher.rend.0      : Uline Bold fg5
URxvt*depth               : 32
URxvt*buffered            : true
URxvt*shading             : 30
URxvt*fading              : 10%
URxvt*internalBorder      : 5
URxvt*scrollBar           : false
URxvt*pointerBlank        : true
URxvt*urlLauncher         : /usr/bin/firefox
URxvt*matcher.pattern.1   : \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt*matcher.button      : 1
URxvt*termName            : rxvt-unicode-256color
URxvt*iso14755            : false
URxvt*iso14755_52         : false
URxvt*colorUL             : #4682B4

URxvt*resize-font.smaller : C-Down
URxvt*resize-font.bigger  : C-Up

! special
*.foreground:   #919191
*.background:   #000000
*.cursorColor:  #919191

! black
*.color0:       #303030
*.color8:       #5e5e5e

! red
*.color1:       #c10023
*.color9:       #ff8781

! green
*.color2:       #a83334
*.color10:      #ffa095

! yellow
*.color3:       #b51d2c
*.color11:      #ff948b

! blue
*.color4:       #8c4b46
*.color12:      #feb2aa

! magenta
*.color5:       #7d534f
*.color13:      #ebbbb5

! cyan
*.color6:       #9a413d
*.color14:      #ffaaa0

! white
*.color7:       #c6c6c6
*.color15:      #ffffff


rofi*modi              : run,ssh
rofi*color-enabled     : true
rofi*color-window      : #000000, #000000, #000000
rofi*color-normal      : #000000, #9e9e0d, #000000, #000000, #5e5e5e
rofi*color-active      : #000000, #4b4b4b, #000000, #000000, #4b4b4b
rofi*color-urgent      : #000000, #4b4b4b, #000000, #000000, #4b4b4b
rofi*terminal          : urxvt
rofi*ssh-client        : ssh
rofi*ssh-command       : {terminal} -e {ssh-client} {host}
rofi*run-command       : {cmd}
rofi*run-shell-command : {terminal} -e {cmd}
